{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "graphql",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "python",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "vue",
    "yaml"
  ],
